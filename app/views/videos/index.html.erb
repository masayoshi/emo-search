<div class="row">
  <div class="span12">
    <div class="page-header">
      <h1>Listing Videos</h1>
    </div>
    <%= render 'form' if current_user.present? %>
    <% if params[:tag] %>
      <p>Tagged with : <span class="label"><%= params[:tag] %><span></p>
    <% end %>
    <%= render @videos %>
    <%= will_paginate @videos %>
  </div>
</div>
